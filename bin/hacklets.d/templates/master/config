[core]
    bare = false
    worktree = ../../
    editor = ~/bin/editor
[color]
    ui = true
[alias]
    wdiff = diff --color-words
    last = log -1 -p HEAD
    st = status
    co = checkout
    ci = commit
    br = branch
    sm = submodule
    cp = cherry-pick
[mergetool]
    keepBackup = false
[merge]
    tool = mergeeditor
[mergetool "mergeeditor"]
    cmd = ~/bin/mergeeditor -f -d -c \"wincmd J\" \"$MERGED\" \"$LOCAL\" \"$BASE\" \"$REMOTE\"
